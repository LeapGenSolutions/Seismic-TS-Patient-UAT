{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/static/js/*",
      "/static/css/*",
      "/static/media/*",
      "/assets/*",
      "*.json"
    ]
  },
  "mimeTypes": {
    ".html": "text/html",
    ".js": "application/javascript",
    ".css": "text/css",
    ".json": "text/json"
  }
}
